[flake8]
exclude =
    .git
    __pycache__
    __init__.py
    setup.py
    example_usage.py
    .eggs
    build
    explabox/test
extend-ignore =
    D101
    D205
    D400
    D401
    D413
    E127
    W291
max-line-length = 120
per-file-ignores =
    __init__.py: F401
select = C,E,F,W,B,B950,D,RST
